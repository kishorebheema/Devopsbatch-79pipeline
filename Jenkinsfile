  agent any
  stages {
    stage('dev') {
      steps {
        echo 'this is dev stage'
      }
    }

    stage('Build') {
      steps {
        echo 'this is build stage'
      }
    }

    stage('Test') {
      steps {
        echo 'this is test stage'
      }
    }

    stage('UAT') {
      steps {
        echo 'this is UAT stage'
      }
    }

    stage('Deploy') {
      steps {
        echo 'this is deploy stage'
      }
    }

    stage('operate') {
      steps {
        echo 'this is operate stage'
      }
    }

  }
}